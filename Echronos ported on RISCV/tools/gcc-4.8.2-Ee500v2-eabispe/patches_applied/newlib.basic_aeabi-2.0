+	blt-	cr1,	L(log_return_nan)		# log(-x) = NaN
+	bgelr-	cr5				# log(+Inf/NaN) = +Inf/NaN
+
+	li	k,	0			# k = 0
+	bge+	cr6,	L(log_argument_reduction)
+
+	li	k,	-54			# k = -54
+
+	cmpwi	cr0,	hx,	0
+	bne+	cr0,	L(log_denorm_small_shift)
+
+	cntlzw	r5,	lx			# r5 = N = number of leading 0s
+	cmpwi	cr5,	r5,	11
+	blt+	cr5,	L(log_denorm_middle_shift)
