+	efdmul	w,	z,	z		# w = z * z
+
+	lis	r5,	0x3FC2
+	ori	r5, r5,	0xF112
+	lis	r6,	0xDF3E
+	ori	r6, r6,	0x5244
+	evmergelo	t,	r5,r6		# t = l7
+	efdmul	x,	w,	t		# x = w * l7
