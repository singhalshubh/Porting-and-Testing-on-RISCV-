+	mr	r3,	fhi
+	mr	r4,	flo
+	mr	r5,	fhi
+	mr	r6,	flo
+	bl	JUMP_TARGET(__muldf3)
+	lis	r5,	0x3fe0
+	li	r6,	0
+	bl	JUMP_TARGET(__muldf3)
