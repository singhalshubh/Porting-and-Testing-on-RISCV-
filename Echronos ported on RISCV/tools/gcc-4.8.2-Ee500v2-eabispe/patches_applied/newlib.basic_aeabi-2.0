+#define t r7
+#define k r8
+#define z r9
+#define w r10
+#define s r11
+#define f r12
+
+	lis	r5,	0x7ff0
+	lis	r6,	0x0010
+	clrlwi	r0,	hx,	1		# |hx|
+	or.	r0,	r0,	lx		# if x = 0
+
+	cmpwi	cr1,	hx,	0		# sign in cr1
