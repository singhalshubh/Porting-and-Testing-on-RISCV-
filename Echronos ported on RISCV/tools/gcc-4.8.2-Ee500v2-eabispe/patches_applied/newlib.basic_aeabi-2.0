diff -ruN newlib-2.0.0-orig/newlib/libc/aeabi/aeabi_atexit.c newlib-2.0.0/newlib/libc/aeabi/aeabi_atexit.c
--- newlib-2.0.0-orig/newlib/libc/aeabi/aeabi_atexit.c	1969-12-31 18:00:00.000000000 -0600
+++ newlib-2.0.0/newlib/libc/aeabi/aeabi_atexit.c	2013-01-22 16:12:51.793805002 -0600
@@ -0,0 +1,10 @@
+#include <stdlib.h>
+
+/* Register a function to be called by exit or when a shared library
+   is unloaded.  This routine is like __cxa_atexit, but uses the
+   calling sequence required by the ARM EABI.  */
+int
+__aeabi_atexit (void *arg, void (*func) (void *), void *d)
+{
+  return __cxa_atexit (func, arg, d);
+}
