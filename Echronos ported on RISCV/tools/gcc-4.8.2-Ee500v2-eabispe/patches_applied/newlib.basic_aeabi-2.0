+	ori	r5, r5,	0x9999
+	lis	r6,	0x9997
+	ori	r6, r6,	0xFA04
+	evmergelo	t,	r5,r6		# t = l2
+	efdadd	x,	x,	t		# x = l2 + w*(l4+w*l6)
+	efdmul	x,	x,	w		# x = w * (l2+w*(l4+w*l6))
