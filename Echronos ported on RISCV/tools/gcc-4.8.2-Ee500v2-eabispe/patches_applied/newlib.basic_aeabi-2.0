+	ori	r5, r5,	0x4924
+	lis	r6,	0x9422
+	ori	r6, r6,	0x9359
+	bl	JUMP_TARGET(__adddf3)			# l3+w*(l5+w*l7)
+
+	mr	r5,	whi
+	mr	r6,	wlo
+	bl	JUMP_TARGET(__muldf3)			# w*(l3+w*(l5+w*l7))
+
+	lis	r5,	0x3FE5
+	ori	r5, r5,	0x5555
+	lis	r6,	0x5555
+	ori	r6, r6,	0x5593
+	bl	JUMP_TARGET(__adddf3)			# l1+w*(l3+w*(l5+w*l7))
+
+	mr	r5,	zhi
+	mr	r6,	zlo
+	bl	JUMP_TARGET(__muldf3)			# t2 = z*(l1+w*(l3+w*(l5+w*l7)))
+	mr	zhi,	r3
+	mr	zlo,	r4			# store temporary in z
+
+	mr	r3,	whi
+	mr	r4,	wlo
+	lis	r5,	0x3FC3
+	ori	r5, r5,	0x9A09
+	lis	r6,	0xD078
+	ori	r6, r6,	0xC69F
+	bl	JUMP_TARGET(__muldf3)			# w*l6
+
+	lis	r5,	0x3FCC
+	ori	r5, r5,	0x71C5
+	lis	r6,	0x1D8E
+	ori	r6, r6,	0x78AF
+	bl	JUMP_TARGET(__adddf3)			# l4+w*l6
+
+	mr	r5,	whi
+	mr	r6,	wlo
+	bl	JUMP_TARGET(__muldf3)			# w*(l4+w*l6)
+
+	lis	r5,	0x3FD9
+	ori	r5, r5,	0x9999
+	lis	r6,	0x9997
+	ori	r6, r6,	0xFA04
+	bl	JUMP_TARGET(__adddf3)			# l2+w*(l4+w*l6)
+
+	mr	r5,	whi
+	mr	r6,	wlo
+	bl	JUMP_TARGET(__muldf3)			# t1 = w*(l2+w*(l4+w*l6))
+
+	mr	r5,	zhi
+	mr	r6,	zlo
+	bl	JUMP_TARGET(__adddf3)			# R = t1+t2
