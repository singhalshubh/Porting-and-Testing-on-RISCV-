+	mr	r4,	dlo
+	lis	r5,	0x3dea
+	ori	r5, r5,	0x39ef
+	lis	r6,	0x3579
+	ori	r6, r6,	0x3c76
+	bl	JUMP_TARGET(__muldf3)			# dk*ln2LO
+
