+	evmergelohi	t,	f,	f
+	or.	t,	t,	f
+
+	cmpwi	cr5,	r5,	3		# if ((0x000fffff&(2+hx)) < 3)
+
+	bge+	cr5,	L(log_calculation)
+	beq-	cr0,	L(log_zero_f)
+
+	lis	r5,	0x3FD5
+	ori	r5, r5,	0x5555
+	lis	r6,	0x5555
+	ori	r6, r6,	0x5555
