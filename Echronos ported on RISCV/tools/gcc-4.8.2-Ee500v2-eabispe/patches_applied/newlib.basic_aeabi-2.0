+	srw	hx,	lx,	r6
+	clrlwi	hx,	hx,	12		# remove leading one
+	addi	r5,	r5,	+21
+	slw	lx,	lx,	r5
+	b	L(log_denorm_calc_exp)
+
+L(log_denorm_small_shift):
+	cntlzw	r5,	hx			# r5 = N = number of leading 0s
+	addi	r5,	r5,	-11		# discard zeroes in exp
+	slw	hx,	hx,	r5
